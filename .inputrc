set input-meta on
set output-meta on
set convert-meta off
#set editing-mode emacs 
set editing-mode vi
#set keymap emacs 
set keymap vi-insert 
"\C-A": beginning-of-line
"\e[1~": beginning-of-line
"\e[H": beginning-of-line
"\C-b": beginning-of-line
"\e[4~": end-of-line
"\e[F": end-of-line
"\C-e": end-of-line
"\C-E": end-of-line
"\e[3~": delete-char
"\^D": delete-char
"\e^[d": kill-word 
"\C-p": history-search-backward
"\C-n": history-search-forward
"\e[A": previous-history
"\e[B": next-history
"\e[C": forward-char
"\e[D": backward-char
"\C-k": kill-line
"\C-u": unix-line-discard
"\C-w": unix-filename-rubout
"\e^[b": backward-word
"\e^[f": forward-word
